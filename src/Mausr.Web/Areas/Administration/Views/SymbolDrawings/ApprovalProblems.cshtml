@model List<SymbolDrawing>
@{
	ViewBag.Title = "Fix approvals";
	var drawingModel = new SymbolDrawingPartialViewModel() {
		ImageSise = 128,
		PenSizePerc = 4,
	};
}

<h1>@ViewBag.Title</h1>


@Html.ActionLink("Fix approval problems", MVC.Administration.SymbolDrawings.FixApprovalProblems())

@if (Model.Count == 0) {
	<p>No problems detected.</p>
}
else {
	<div class="row">
		@foreach (var drawing in Model) {
			drawingModel.SymbolDrawing = drawing;
			<div class="pull-left small">
				@{
					Html.RenderPartial(MVC.Administration.Shared.Views.Symbol_partial, drawingModel);
				}
			</div>
		}
	</div>
}