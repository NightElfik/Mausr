@model IEnumerable<ApplicationUser>

@{
	ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
	@Html.ActionLink("Create New", "Create")
</p>
<table class="table">
	<tr>
		<th>@Html.DisplayNameFor(m => m.UserName)</th>
		<th>@Html.DisplayNameFor(m => m.Email)</th>
		<th>@Html.DisplayNameFor(m => m.EmailConfirmed)</th>
		<th>@Html.DisplayNameFor(m => m.Roles)</th>
		<th>@Html.DisplayNameFor(m => m.LockoutEndDateUtc)</th>
		<th>@Html.DisplayNameFor(m => m.LockoutEnabled)</th>
		<th>@Html.DisplayNameFor(m => m.AccessFailedCount)</th>
		<th>@Html.DisplayNameFor(m => m.SymbolDrawings)</th>
		<th></th>
	</tr>

	@foreach (var user in Model) {
		<tr>
			<td>@Html.DisplayFor(m => user.UserName)</td>
			<td>@Html.DisplayFor(m => user.Email)</td>
			<td>@Html.DisplayFor(m => user.EmailConfirmed)</td>
			<td>@string.Join(", ", user.Roles.Select(r => r.RoleId))</td>
			<td>@Html.DisplayFor(m => user.LockoutEndDateUtc)</td>
			<td>@Html.DisplayFor(m => user.LockoutEnabled)</td>
			<td>@Html.DisplayFor(m => user.AccessFailedCount)</td>
			<td>@string.Join(", ", user.SymbolDrawings.Count)</td>
			<td>
				@Html.ActionLink("Edit", "Edit", new { id = user.Id }) |
				@Html.ActionLink("Details", "Details", new { id = user.Id }) |
				@Html.ActionLink("Delete", "Delete", new { id = user.Id })
			</td>
		</tr>
	}

</table>
